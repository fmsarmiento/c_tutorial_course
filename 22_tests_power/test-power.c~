#include<stdlib.h>
#include<stdio.h>
unsigned power (unsigned x, unsigned y);

int main(void) {
  // try: 
  if(power(2,2) != 4) {
    exit(EXIT_FAILURE);
  }
  if(power(2,2) != 4) {
    exit(EXIT_FAILURE);
  }
  if(power(2,2) != 4) {
    exit(EXIT_FAILURE);
  }
  if(power(2,2) != 4) {
    exit(EXIT_FAILURE);
  }
  if(power(2,2) != 4) {
    exit(EXIT_FAILURE);
  }
  return EXIT_SUCCESS;
}
